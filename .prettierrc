{
    "$schema": "https://json.schemastore.org/prettierrc",
    "arrowParens": "always",
    "bracketSpacing": true,
    "plugins": ["@trivago/prettier-plugin-sort-imports"],
    "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^@/app/(.*)$",
    "^@/components/(.*)$",
    "^@/features/(.*)$",
    "^@/hooks/(.*)$",
    "^@/lib/(.*)$",
    "^@/utils/(.*)$",
    "^@/routes/(.*)$",
    "^@/pages/(.*)$",
    "^@/styles/(.*)$",
    "^@/assets/(.*)$",
    "^./(.*)$",
    "^../(.*)$"
  ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "printWidth": 80,
    "singleQuote": true,
    "proseWrap": "always"
  }